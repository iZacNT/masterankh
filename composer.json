{
    "name": "genieping/genieping",
    "description": "Genie ping",
    "keywords": ["genieping", "yii2"],
    "homepage": "https://genieping.com/",
    "type": "project",
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2.0",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "yiisoft/yii2": "~2.0.39",
        "yiisoft/yii2-bootstrap": "~2.0.7",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-jui": "^2.0",
        "kartik-v/yii2-widgets": "^3.4",
        "kartik-v/yii2-grid": "*",
        "boundstate/yii2-mailgun": "^1.3",
        "kriswallsmith/buzz": "^1.1",
        "nyholm/psr7": "^1.3",
        "chuyskywalker/rolling-curl": "^3.1",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "wanfeiyy/dd": "*",
        "himiklab/yii2-recaptcha-widget": "^2.1",
        "yiisoft/yii2-authclient": "~2.1.7"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
